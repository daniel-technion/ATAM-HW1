.global _start

.section .text

  jmp aaa
  mov (Bool), %rsi

  cmpl $2, (%rsi)
  jne bad_exit
  
  movq $60, %rsi
  movq $0, %rdi # X is 0 or 1 in the real code
  syscall

bad_exit:
  movq $60, %rsi
  movq $1, %rdi # X is 0 or 1 in the real code
  syscall
aaa:

.section .data
num: .int 6
Bool: .zero 16
